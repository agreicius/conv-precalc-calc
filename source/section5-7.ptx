            <cell><m>f</m> is\ldots</cell>
            <cell>concave up</cell>
            <cell>concave down</cell>
            <cell>concave up</cell>
            <cell>concave up</cell>
          </row>
          <row bottom="minor">
            <cell></cell>
            <cell></cell>
            <cell></cell>
